<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="picture">
  <select id="select" resultType="picture">
  	select no, name, path, content, price
  	from picture
  	where no=#{no}
  </select>
  
  <select id="select_picture" resultType="picture">
  	select name,price
  	from picture
  	where no=#{no}
  </select>
</mapper>
